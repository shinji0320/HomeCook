<h1>プロフィールの編集</h1>
<%= form_with model:@user, local:true do |f| %>
  <h4>Name</h4>
  <%= f.text_field :name %>
  <%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.png"%>
  <h4>ProfileImage</h4>
  <%= f.attachment_field :profile_image %>
  <%= f.submit "変更を保存" %>
<% end %>

<%= link_to "戻る", users_path %>