<div class="container">
  <div class="row">
    <div class="col">

      <p><%= current_user.name %></p>
      <p><%= attachment_image_tag current_user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.png", size:'100x100' %></p>
      <p><%= link_to "プロフィールの編集", edit_user_path(current_user) %></p>

      <% @users.each do |user| %>
        <p><%= link_to user.name, user_path(user) %></p>
      <% end %>
    </div>
  </div>
</div>